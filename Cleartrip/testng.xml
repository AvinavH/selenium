<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite guice-stage="DEVELOPMENT" name="Automation Test Suite for Cleartrip">
  <test thread-count="5" verbose="2" name="Automation of cleartrip.com in Chrome">
  <parameter name="Browser" value="Chrome" />
  <parameter name="url" value="https://www.cleartrip.com/flights" />
    <classes>
      <class name="com.cleartrip.test.searchFlightTest"/>
    </classes>
  </test> <!-- test -->
  
 <test thread-count="5" verbose="2" name="Automation of cleartrip.com in Firefox">
  <parameter name="Browser" value="Firefox" />
  <parameter name="url" value="https://www.cleartrip.com/flights" />
    <classes>
      <class name="com.cleartrip.test.searchFlightTest"/>
    </classes>
  </test> 
</suite> <!-- Suite -->
